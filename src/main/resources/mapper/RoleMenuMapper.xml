<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shpun.mapper.RoleMenuMapper">

    <insert id="insert" parameterType="com.shpun.model.RoleMenu" useGeneratedKeys="true" keyProperty="id">
        insert into role_menu
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="roleId != null">
                role_id,
            </if>
            <if test="menuId != null">
                menu_id,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="roleId != null">
                #{roleId},
            </if>
            <if test="menuId != null">
                #{menuId},
            </if>
        </trim>
    </insert>

</mapper>